#-*-coding:latin1-*-
import math
print("Entre com tres numeros inteiros")
a = int(input("A primeira variavel: "))
b = int(input("A segunda variavel: "))
c = int(input("A terceira variavel: "))
def roll(a,b,c):
    min = a
    if b<min:
        min=b
    if c<min:
        min=c
    return min
print("Menor numero: ",roll(a,b,c))
