#-*-coding:latin1-*-
import math
print("Entre com tres numeros inteiros e positivos")
a = int(input("A primeira variavel: "))
b = int(input("A segunda variavel: "))
c = int(input("A terceira variavel: "))
if a<b+c:
    print("A area equivale a ", (a * b * c))
else:
    print("Os valores de entrada sao ", (a,b,c))
